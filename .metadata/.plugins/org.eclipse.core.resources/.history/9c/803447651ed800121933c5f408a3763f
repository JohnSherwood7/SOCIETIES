double andom_binomial(int n, double p)
{
    return randBinomial(n, p);
}

int main() {
	vector<int> resHeld;
	for (int i = 0; i < 10; i++) {
		resHeld.push_back(i);
	}
	for (int i = 0; i < resHeld.size(); i++) {
		if (resHeld > 0) {
			resHeld[i] -= random_binomial(resHeld[i],
	                                     1.0 / 3);
	}
}


